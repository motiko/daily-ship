{
  "version": "0.0.0",
  "name": "git push Every Day",
  "manifest_version": 2,
  "description": "Track your coding habbits",
  "author": "Moti Korets",
  "browser_action": {
    "default_title": "Hack code every day",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/128.png",
    "48": "img/128.png",
    "128": "img/128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [ "storage", "https://github.com/*", "identity" ],
  "content_security_policy": "default-src 'self'; script-src 'self' https://ssl.google-analytics.com;"
}
